<div id ="add_resume_modal" class="modal" >
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&Close;</button>
        <h4 class="modal-title">Tag Your Resume</h4>
      </div>
      <div class="modal-body" >
        <div class="modal-body">

          <ul class="checkbox" id="tag_checkbox">
            <li><input type="checkbox"> <label>Software Development</label></li>

            <li><input type="checkbox"> <label>Marketing</label></li>
            <li><input type="checkbox"> <label>Accounting</label></li>

            <li><input type="checkbox"> <label>Human Resource</label></li>
            <li><input type="checkbox"> <label>Design</label></li>

            <li><input type="checkbox"> <label>Consulting</label></li>
          </ul>

        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
        <button type="button" class="btn btn-primary" onclick="saveTags()">Save</button>
      </div>
    </div>
  </div>
</div>




