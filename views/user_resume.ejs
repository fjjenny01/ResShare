<div id ="add_resume_modal" class="modal" >
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&Close;</button>
        <h4 class="modal-title">Tag Your Resume</h4>
      </div>
      <div class="modal-body" >
        <div class="modal-body">

          <ul class="checkbox" id="tag_checkbox">
            <li><input type="checkbox"> <label>Software Development</label></li>

            <li><input type="checkbox"> <label>Marketing</label></li>
            <li><input type="checkbox"> <label>Accounting</label></li>

            <li><input type="checkbox"> <label>Human Resource</label></li>
            <li><input type="checkbox"> <label>Design</label></li>

            <li><input type="checkbox"> <label>Consulting</label></li>
          </ul>

        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
        <button type="button" class="btn btn-primary" onclick="saveTags()">Save</button>
      </div>
    </div>
  </div>
</div>




<div id ="share_resume_modal" class="modal" >
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&Close;</button>
        <h4 class="modal-title">Share Your Resume</h4>
      </div>
      <div class="modal-body" >
        <div class="modal-body">

          <!--<div class="form-horizontal">-->
            <!--<div class="form-group">-->

          <div class="form-group">
            <!--<label for="#subject">Subject</label>-->
            <label id="fileName"></label> <br>
            Subject:
            <input type="text" id="subject" placeholder="Subject" size="55"/>
          </div>


          <div class="form-group">
            <textarea class="form-control" rows="3" id="content"></textarea>
          </div>


        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
        <button type="button" class="btn btn-primary" onclick="commitShare()">Share</button>
      </div>
    </div>
  </div>
</div>


<div class="container">
  <div class="bs-docs-section">
    <div class="col-xs-3">


      <div class="container">
        <img src="/images/default_profile.jpg" class="img-circle" alt="Cinque Terre" width="200" height="180">
      </div>

      <table class="table table table-hover " id="myresumelist">
        <thead>
        <tr>
          <th>#</th>
          <th>Resume List</th>
        </tr>
        </thead>
        <tbody class="active" id="myresumelistbody">

        </tbody>
      </table>
    </div>


    <div class="col-xs-9">


      <input type="file" id="add_btn" size="char" onchange="addResume()">Choose a new resume to upload</input>

      <a href="#" class="btn btn-primary" onclick="uploadResume()" id="upload_resume">Upload Resume</a>
      <a href="#" class="btn btn-primary"  onclick="deleteResume()" id="delete_resume">Delete Resume</a>
      <a href="#" class="btn btn-primary"  onclick="shareResume()" id="share_resume">Share Resume</a>

      <br />
      <iframe id="organigram-iFrame" src="http://docs.google.com/gview?url=https://s3.amazonaws.com/resumefiles/resume_JINFANG.pdf&embedded=true" onmousedown="return false;" onselectstart="return false;"
